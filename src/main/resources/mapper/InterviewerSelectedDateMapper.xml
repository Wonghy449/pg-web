<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pixel.pgback.mapper.InterviewerSelectedDateMapper">
    <resultMap id="BaseResultMap" type="com.pixel.pgback.entity.InterviewerSelectedDate">
        <result column="id" jdbcType="INTEGER" property="id"/>
        <result column="interviewer_id" jdbcType="INTEGER" property="interviewerId"/>
        <result column="selected_date" jdbcType="VARCHAR" property="selectedDate"/>
    </resultMap>
    <insert id="insertSelective" parameterType="com.pixel.pgback.entity.InterviewerSelectedDate">
        insert into interviewer_selected_date
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="interviewerId != null">
                interviewer_id,
            </if>
            <if test="selectedDate != null">
                selected_date,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id,jdbcType=INTEGER},
            </if>
            <if test="interviewerId != null">
                #{interviewerId,jdbcType=INTEGER},
            </if>
            <if test="selectedDate != null">
                #{selectedDate,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>
    <insert id="insertTime">
        insert into interviewer_selected_date (interviewer_id, selected_date) values
        <foreach collection="times" index="index" item="time" separator=",">
            (#{id}, #{time})
        </foreach>
    </insert>
</mapper>